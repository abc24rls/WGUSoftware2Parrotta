<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="188.0" prefWidth="228.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.LoginController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" prefHeight="33.0" prefWidth="254.0" text="Login">
         <font>
            <Font name="SansSerif Regular" size="18.0" />
         </font>
      </Label>
      <VBox layoutX="16.0" layoutY="44.0">
         <children>
            <TextField fx:id="username" alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
               <font>
                  <Font name="SansSerif Regular" size="18.0" />
               </font>
            </TextField>
            <Label fx:id="usernameLbl" alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT" text="Username">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="password" alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font>
            </TextField>
            <Label fx:id="Passwordlbl" alignment="CENTER_RIGHT" nodeOrientation="LEFT_TO_RIGHT" text="Password">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Button fx:id="Loginbtn" alignment="CENTER_RIGHT" layoutX="201.0" layoutY="148.0" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#loginClick" text="Login">
         <font>
            <Font name="SansSerif Regular" size="14.0" />
         </font>
      </Button>
      <Button fx:id="exitbtn" alignment="CENTER_RIGHT" layoutX="214.0" layoutY="4.0" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#exitButton" text="Exit">
         <font>
            <Font name="SansSerif Regular" size="14.0" />
         </font>
      </Button>
      <Label fx:id="userZone" cacheShape="false" centerShape="false" focusTraversable="false" layoutX="9.0" layoutY="161.0" scaleShape="false" text="Zone" />
   </children>
   <padding>
      <Insets right="10.0" />
   </padding>
</AnchorPane>
